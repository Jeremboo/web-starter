{
  "name": "web-starter",
  "version": "1.0.0",
  "description": "My organization for web developpement with React and Webpack.",
  "main": "app/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jeremboo/web-starter/tree/webpack-react"
  },
  "scripts": {
    "start": "webpack-dev-server --inline --hot --port 3333 --config config/webpack.config.js --content-base public --history-api-fallback",
