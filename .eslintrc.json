{
  "extends": "airbnb",
  "globals": {
    "app": true,
    "window": true,
    "document": true,
    "describe": true,
    "it": true
  },
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "generators": true,
    "restParams": true,
    "spread": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
